{
  "name": "vue-preview-imgs",
  "description": "A picture preview plugin for the Vue2",
  "repository": {
    "type": "git",
    "url": "https://github.com/hzpeng57/vue-preview-imgs"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "version": "lerna version --no-commit-hooks --no-push --conventional-commits",
    "version:force": "npm run version -- --force-publish",
    "publish": "lerna publish from-package --ignore-prepublish --yes",
    "clean": "lerna clean"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "lerna": "^4.0.0",
    "prettier": "2.3.2",
    "typescript": "^4.3.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0"
  },
  "license": "MIT"
}
